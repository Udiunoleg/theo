---
layout: none
---
[
{% assign pages = site.pages | where_exp: "p", "p.title != nil" | where: "lang", "en" %}
{% for page in pages %}
  {
    "title": "{{ page.title | escape }}",
    "url": "{{ site.baseurl }}{{ page.url }}",
    "content": {{ page.content | markdownify | strip_html | jsonify }}
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]
